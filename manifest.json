{
    "manifest_version": 3,
    "name": "Upwork Auto Reload",
    "version": "1.0",
    "description": "Auto reload Upwork Find Work page every 20 seconds until stopped.",
    "permissions": ["storage"],
    "content_scripts": [
      {
        "matches": ["https://www.upwork.com/nx/find-work/most-recent"],
        "js": ["content.js"]
      }
    ]
  }
  